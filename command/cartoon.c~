#include "stdio.h"

void clearScreen()
{
	int i;
	disp_pos=0;
	for(i=0;i<80*25;i++)
	{
		disp_str(" ");
	}
	disp_pos=0;
	
}

int main(int argc, char * argv[])
{
	clearScreen();
	milli_delay(6000);
	disp_str("\n");
	disp_str(">>       Welcome to +1's system! Press [left] or [right] to switch tty.       <<\n");
